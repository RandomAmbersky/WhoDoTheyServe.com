.navbar.navbar-fixed-bottom.bg-transparent.seo-remove
  ul.nav.social
    li.dropup
      a(href='#', data-toggle='dropdown').dropdown-toggle.fe.fe-shareable
        small Share this page
        b.caret
      ul.dropdown-menu
        li
          p.title
        li.divider
        li
          a.email(target='_blank').fe.fe-mail
            small.text Email
        li
          a.twitter(target='_blank' title='Tweet').fe.fe-twitter
            small.text Twitter
        li
          a.facebook(target='_blank').fe.fe-facebook-squared
            small.text Facebook
        li
          a.gplus(target='_blank').fe.fe-gplus
            small.text Google +1
  ul.nav.pull-right
    li.dropup
      a(href='#', data-toggle='dropdown').dropdown-toggle.fe.fe-brush
        small Switch theme
        b.caret
      ul.dropdown-menu.themes
        li(data-theme-id='dark')
          a Dark
        li(data-theme-id='light')
          a Light
    li
      a(href='https://github.com/dizzib/WhoDoTheyServe.com' target='_blank').fe.fe-github-circled
        small Source code on Github
    li
      a(href='mailto:&#100;&#105;&#122;&#122;&#105;&#098;&#048;&#064;&#103;&#109;&#097;&#105;&#108;&#046;&#099;&#111;&#109;').fe.fe-mail
        small Email site author
