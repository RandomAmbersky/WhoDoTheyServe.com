include _mixin

.latest
  legend Latest Updates
  ul.items.unstyled
    li.item
      span.entity
        span._type-edge
          +edge
        span._type-map
          a.link
            .glyph.fe.fe-share
            span.name
        span._type-node
          +node-name
          +node-tags
        span._type-note
          a(title='note').glyph.fe.fe-comment
          span.note
      +meta-compact
