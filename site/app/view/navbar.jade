      i(class='fa fa-user') Account 
  li(active='^user/signup').seo-remove.pull-right.signed-in-admin
    a(href='#/user/signup') Signup
  li(active='^user(s|/[0-9a-f]+)$').seo-remove.pull-right.signed-in
