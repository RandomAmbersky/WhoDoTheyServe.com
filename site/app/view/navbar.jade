      i(class='fa fa-info-circle') About
  li(active='^node')
    a(href='#/nodes')
      i(class='fa fa-male') Actors
  li(active='^edge')
    a(href='#/edges')
      i(class='fa fa-chain') Connections
  li(active='^$|^graph').seo-remove
    a(href='#/graph')
